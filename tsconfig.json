{
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@utils/*": ["src/utils/*"],
      "@contexts/*": ["src/contexts/*"],
      "@configNavigation/*": ["src/navigation/*"],
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@reduxConfig/*": ["src/redux_configuration/*"],
      "@modules/*": ["src/modules/*"],
      "@mainAssets/*": ["./assets/*"],
      "@comman/*": ["src/comman/*"],
      "@services/*": ["src/services/*"],
      "@hooks/*": ["src/hooks/*"],
      "@constants/*": ["src/constants/*"],
      "@helpers/*": ["src/helpers/*"]
    }
  },
  "extends": "@tsconfig/react-native/tsconfig.json"
}
